{
  "name": "image-processing-fronted",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend for Handling Images from API, with option to set the width & height & catching images thumbs",
  "scripts": {
    "start": "cross-env NODE_ENV=dev react-scripts start",
    "build": "cross-env NODE_ENV=production react-scripts build",
    "test:ci": "cross-env NODE_ENV=test react-scripts test  --env=jsdom --watchAll=false --ci --detectOpenHandles --forceExit",
    "test": "cross-env NODE_ENV=test react-scripts test --watchAll",
    "eject": "react-scripts eject",
    "deploy": "chmod +x bin/deploy.sh && bin/deploy.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "cross-env": "^7.0.3",
    "formik": "^2.2.9",
    "moment": "^2.29.2",
    "react": "^18.0.0",
    "react-bootstrap": "^2.2.3",
    "react-date-picker": "^8.4.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.29.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "sweetalert2": "^11.4.8",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.3",
